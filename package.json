{
  "name": "corsproxyserver",
  "version": "1.0.0",
  "description": "CORS Proxy server.",
  "author": "Jasurbek Shomaqsudov",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "mocha",
    "lint": "eslint .",
    "build": "babel src -d dist",
    "prestart": "npm run build"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "printWidth": 200,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "auto",
    "bracketSameLine": true
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "@vercel/node": "^3.1.5",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  }
}
