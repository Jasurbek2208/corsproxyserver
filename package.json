{
  "name": "corsproxyserver",
  "version": "1.0.0",
  "description": "CORS Proxy server.",
  "author": "Jasurbek Shomaqsudov",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "bun run index.ts",
    "start": "bun run dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "printWidth": 200,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "auto",
    "bracketSameLine": true
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  }
}
